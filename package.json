{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "pretest": "ENV=test db-migrate db:drop storefront_backend_project_test && db-migrate db:create storefront_backend_project_test && ENV=test db-migrate --env test up",
    "test": "ENV=test jasmine-ts \"src/tests/**/*.ts\"",
    "posttest": "db-migrate db:drop storefront_backend_project_test",
    "tsc": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "setup-database": "db-migrate db:create storefront_backend_project && db-migrate db:create storefront_backend_project_test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "body-parser": "^1.19.0",
    "typescript": "^4.1.3",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
